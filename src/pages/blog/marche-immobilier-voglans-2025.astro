---
import Base from '@/layouts/Base.astro';
import HeroUltra from '@/components/ui/HeroUltra.astro';
import BadgeUltra from '@/components/ui/BadgeUltra.astro';
import ButtonUltra from '@/components/ui/ButtonUltra.astro';
import CardUltra from '@/components/ui/CardUltra.astro';
import {
  ArrowLeft,
  TrendingUp,
  MapPin,
  Calculator,
  Users,
  Award,
  CheckCircle,
  Building,
  Home,
  Calendar,
  Clock
} from 'lucide-react';
import { formatDate } from '@/lib/utils';

const publishDate = new Date('2025-01-15');

// Article Schema pour SEO
const articleSchema = {
  "@context": "https://schema.org",
  "@type": "Article",
  "headline": "Le marché immobilier à Voglans en 2025",
  "description": "Analyse complète du marché immobilier à Voglans et en Savoie en 2025. Tendances, prix, et opportunités d'investissement.",
  "image": "https://agencevoglans.fr/images/blog/marche-voglans-2025.jpg",
  "author": {
    "@type": "Organization",
    "name": "L'Agence de Voglans",
    "url": "https://agencevoglans.fr"
  },
  "publisher": {
    "@type": "Organization",
    "name": "L'Agence de Voglans",
    "logo": {
      "@type": "ImageObject",
      "url": "https://agencevoglans.fr/images/logo-agence-voglans.png"
    }
  },
  "datePublished": "2025-01-15T10:00:00+02:00",
  "dateModified": "2025-01-15T10:00:00+02:00",
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "https://agencevoglans.fr/blog/marche-immobilier-voglans-2025"
  },
  "articleSection": "Marché immobilier",
  "keywords": ["marché immobilier Voglans", "prix immobilier Savoie 2025", "tendances immobilier Voglans", "opportunités immobilier"]
};
---

<Base
  title="Le marché immobilier à Voglans en 2025 - L'Agence de Voglans"
  description="Analyse complète du marché immobilier à Voglans et en Savoie en 2025. Tendances, prix, et opportunités d'investissement."
>
  <Fragment slot="head">
    <script type="application/ld+json" set:html={JSON.stringify(articleSchema)} />
  </Fragment>
  <!-- Hero Section Ultra-Moderne -->
  <HeroUltra>
    <div slot="badge">
      <BadgeUltra text="Analyse 2025" />
    </div>

    <div slot="title">
      Le marché immobilier<br>
      <span class="font-semibold bg-gradient-to-r from-primary to-primary/70 bg-clip-text text-transparent">à Voglans</span>
    </div>

    <div slot="subtitle">
      Analyse complète des tendances, prix et opportunités d'investissement<br>
      dans notre commune et en Savoie pour 2025.
    </div>

    <div slot="cta">
      <ButtonUltra href="/estimation" text="Estimer mon bien" />
    </div>
  </HeroUltra>

  <!-- Article Content Ultra-Moderne -->
  <section class="py-24 bg-white relative overflow-hidden">
    <!-- Fond décoratif subtil -->
    <div class="absolute inset-0 bg-gradient-to-br from-primary/5 via-transparent to-primary/10"></div>

    <div class="container mx-auto px-4 relative z-10">
      <div class="max-w-4xl mx-auto">
        <!-- Navigation retour -->
        <div class="mb-12">
          <a href="/blog" class="inline-flex items-center gap-3 bg-white/80 backdrop-blur-xl border border-primary/20 text-primary px-6 py-3 rounded-full text-sm font-semibold shadow-xl shadow-primary/10 hover:scale-105 transition-all duration-300 group">
            <ArrowLeft className="w-4 h-4 group-hover:-translate-x-1 transition-transform duration-300" />
            <span>Retour au blog</span>
          </a>
        </div>

        <!-- Article Header -->
        <header class="mb-16 text-center">
          <div class="inline-flex items-center gap-2 bg-primary/10 text-primary px-4 py-2 rounded-full text-sm font-semibold mb-6">
            <Calendar className="w-4 h-4" />
            {formatDate(publishDate)} • 8 min de lecture
          </div>
          <h1 class="text-4xl md:text-5xl font-bold mb-6 text-foreground leading-tight">
            Le marché immobilier à Voglans en 2025
          </h1>
          <p class="text-xl text-muted max-w-3xl mx-auto leading-relaxed">
            Analyse complète des tendances, prix et opportunités d'investissement
            dans notre commune et en Savoie.
          </p>
        </header>

        <!-- Article Content -->
        <div class="space-y-12">
          <!-- Introduction -->
          <CardUltra class="group">
            <div class="text-center">
              <div class="w-24 h-24 bg-gradient-to-br from-primary/20 to-primary/10 backdrop-blur-sm rounded-2xl flex items-center justify-center mx-auto mb-6 border border-primary/20 group-hover:scale-110 transition-transform duration-500">
                <TrendingUp className="w-12 h-12 text-primary" />
              </div>
              <h2 class="text-3xl font-bold text-foreground mb-4 group-hover:text-primary transition-colors duration-300">Une nouvelle agence pour un marché en évolution</h2>
              <p class="text-muted leading-relaxed text-lg">
                L'Agence de Voglans ouvre ses portes en 2025 dans un contexte immobilier particulièrement
                intéressant en Savoie. Notre positionnement 100% local nous permet d'analyser finement
                les évolutions du marché et d'accompagner nos clients avec une expertise terrain unique.
              </p>
            </div>
          </CardUltra>

          <!-- Position géographique -->
          <CardUltra class="group">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-gradient-to-br from-primary/20 to-primary/10 backdrop-blur-sm rounded-2xl flex items-center justify-center flex-shrink-0 border border-primary/20 group-hover:scale-110 transition-transform duration-500">
                <MapPin className="w-8 h-8 text-primary" />
              </div>
              <div class="flex-1">
                <h2 class="text-3xl font-bold text-foreground mb-4 group-hover:text-primary transition-colors duration-300">Voglans : un positionnement géographique exceptionnel</h2>
                <p class="text-muted leading-relaxed text-lg mb-6">
                  Située entre Chambéry et le lac du Bourget, Voglans bénéficie d'un emplacement privilégié
                  qui en fait une commune très recherchée :
                </p>
                <div class="grid gap-4">
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Proximité de Chambéry :</strong>
                      <p class="text-muted mt-1">10 minutes du centre-ville</p>
                    </div>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Accès au lac du Bourget :</strong>
                      <p class="text-muted mt-1">5 minutes des plages</p>
                    </div>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Transports :</strong>
                      <p class="text-muted mt-1">Gare SNCF et autoroutes A41/A43</p>
                    </div>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Services :</strong>
                      <p class="text-muted mt-1">Écoles, commerces, espaces verts</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </CardUltra>

          <!-- Tendances du marché -->
          <CardUltra class="group">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-gradient-to-br from-primary/20 to-primary/10 backdrop-blur-sm rounded-2xl flex items-center justify-center flex-shrink-0 border border-primary/20 group-hover:scale-110 transition-transform duration-500">
                <Calculator className="w-8 h-8 text-primary" />
              </div>
              <div class="flex-1">
                <h2 class="text-3xl font-bold text-foreground mb-4 group-hover:text-primary transition-colors duration-300">Tendances du marché 2025</h2>

                <h3 class="text-2xl font-semibold text-foreground mb-4">Évolution des prix</h3>
                <p class="text-muted leading-relaxed text-lg mb-6">
                  Le marché immobilier savoyard montre une stabilisation après les hausses importantes
                  des années précédentes. À Voglans, nous observons :
                </p>

                <div class="bg-gradient-to-br from-primary/5 to-primary/10 rounded-2xl p-8 border border-primary/10">
                  <h4 class="font-bold text-foreground mb-6 text-center">Prix moyens constatés (estimation 2025)</h4>
                  <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
                    <div class="space-y-3">
                      <h5 class="font-semibold text-primary text-lg">Maisons :</h5>
                      <div class="space-y-2 text-muted">
                        <p>• Avec terrain : 400 000 - 600 000 €</p>
                        <p>• Rénovées : 350 000 - 500 000 €</p>
                      </div>
                    </div>
                    <div class="space-y-3">
                      <h5 class="font-semibold text-primary text-lg">Appartements :</h5>
                      <div class="space-y-2 text-muted">
                        <p>• T3 : 250 000 - 350 000 €</p>
                        <p>• T4/T5 : 350 000 - 450 000 €</p>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </CardUltra>

          <!-- Profils d'acheteurs -->
          <CardUltra class="group">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-gradient-to-br from-primary/20 to-primary/10 backdrop-blur-sm rounded-2xl flex items-center justify-center flex-shrink-0 border border-primary/20 group-hover:scale-110 transition-transform duration-500">
                <Users className="w-8 h-8 text-primary" />
              </div>
              <div class="flex-1">
                <h2 class="text-3xl font-bold text-foreground mb-4 group-hover:text-primary transition-colors duration-300">Profils d'acheteurs</h2>
                <p class="text-muted leading-relaxed text-lg mb-6">
                  Nous identifions trois profils principaux d'acheteurs sur Voglans :
                </p>
                <div class="space-y-4">
                  <div class="flex items-start gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-8 h-8 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <span class="text-primary font-bold">1</span>
                    </div>
                    <div>
                      <strong class="text-foreground">Familles locales :</strong>
                      <p class="text-muted mt-1">Recherche de maisons avec jardin</p>
                    </div>
                  </div>
                  <div class="flex items-start gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-8 h-8 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <span class="text-primary font-bold">2</span>
                    </div>
                    <div>
                      <strong class="text-foreground">Investisseurs :</strong>
                      <p class="text-muted mt-1">Attirés par la proximité du lac et de Chambéry</p>
                    </div>
                  </div>
                  <div class="flex items-start gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-8 h-8 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <span class="text-primary font-bold">3</span>
                    </div>
                    <div>
                      <strong class="text-foreground">Retraités :</strong>
                      <p class="text-muted mt-1">Séduits par le cadre de vie et les services</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </CardUltra>

          <!-- Opportunités d'investissement -->
          <CardUltra class="group">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-gradient-to-br from-primary/20 to-primary/10 backdrop-blur-sm rounded-2xl flex items-center justify-center flex-shrink-0 border border-primary/20 group-hover:scale-110 transition-transform duration-500">
                <Award className="w-8 h-8 text-primary" />
              </div>
              <div class="flex-1">
                <h2 class="text-3xl font-bold text-foreground mb-4 group-hover:text-primary transition-colors duration-300">Opportunités d'investissement</h2>

                <h3 class="text-2xl font-semibold text-foreground mb-4">Secteurs porteurs</h3>
                <p class="text-muted leading-relaxed text-lg mb-6">
                  Plusieurs secteurs de Voglans présentent un potentiel intéressant :
                </p>
                <div class="grid gap-4">
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Centre-bourg :</strong>
                      <p class="text-muted mt-1">Proximité des commerces et services</p>
                    </div>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Secteur lac :</strong>
                      <p class="text-muted mt-1">Attractivité touristique et résidentielle</p>
                    </div>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Zones pavillonnaires :</strong>
                      <p class="text-muted mt-1">Demande familiale soutenue</p>
                    </div>
                  </div>
                </div>

                <h3 class="text-2xl font-semibold text-foreground mb-4 mt-8">Rendements locatifs</h3>
                <p class="text-muted leading-relaxed text-lg mb-6">
                  Les rendements locatifs à Voglans restent attractifs, particulièrement pour :
                </p>
                <div class="grid gap-4">
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Appartements T2/T3 :</strong>
                      <p class="text-muted mt-1">4-5% brut</p>
                    </div>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Maisons familiales :</strong>
                      <p class="text-muted mt-1">3-4% brut</p>
                    </div>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <div>
                      <strong class="text-foreground">Locations saisonnières :</strong>
                      <p class="text-muted mt-1">6-8% brut (réglementation à vérifier)</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </CardUltra>

          <!-- Conseils -->
          <CardUltra class="group">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-gradient-to-br from-primary/20 to-primary/10 backdrop-blur-sm rounded-2xl flex items-center justify-center flex-shrink-0 border border-primary/20 group-hover:scale-110 transition-transform duration-500">
                <Award className="w-8 h-8 text-primary" />
              </div>
              <div class="flex-1">
                <h2 class="text-3xl font-bold text-foreground mb-4 group-hover:text-primary transition-colors duration-300">Conseils de notre équipe locale</h2>

                <h3 class="text-2xl font-semibold text-foreground mb-4">Pour les vendeurs</h3>
                <p class="text-muted leading-relaxed text-lg mb-6">
                  En tant qu'agence 100% locale, nous recommandons :
                </p>
                <div class="grid gap-4 mb-8">
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <p class="text-muted">Mise en valeur des atouts géographiques (lac, montagne)</p>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <p class="text-muted">Présentation soignée et home staging</p>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <p class="text-muted">Prix en adéquation avec le marché local</p>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <p class="text-muted">Accompagnement personnalisé tout au long du processus</p>
                  </div>
                </div>

                <h3 class="text-2xl font-semibold text-foreground mb-4">Pour les acheteurs</h3>
                <div class="grid gap-4">
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <p class="text-muted">Définir précisément ses critères de recherche</p>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <p class="text-muted">Prévoir les frais annexes (notaire, travaux éventuels)</p>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <p class="text-muted">S'appuyer sur une expertise locale pour les négociations</p>
                  </div>
                  <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl">
                    <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                      <CheckCircle className="w-4 h-4 text-primary" />
                    </div>
                    <p class="text-muted">Anticiper l'évolution du secteur choisi</p>
                  </div>
                </div>
              </div>
            </div>
          </CardUltra>

          <!-- Engagement final -->
          <CardUltra class="group">
            <div class="text-center">
              <h2 class="text-3xl font-bold text-foreground mb-4">L'engagement de L'Agence de Voglans</h2>
              <p class="text-muted leading-relaxed text-lg mb-8">
                Notre approche 100% locale et humaine nous permet de vous accompagner avec :
              </p>
              <div class="grid gap-4 mb-8">
                <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl justify-center">
                  <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                    <CheckCircle className="w-4 h-4 text-primary" />
                  </div>
                  <div>
                    <strong class="text-foreground">Expertise terrain :</strong>
                    <p class="text-muted mt-1">Connaissance fine du marché local</p>
                  </div>
                </div>
                <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl justify-center">
                  <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                    <CheckCircle className="w-4 h-4 text-primary" />
                  </div>
                  <div>
                    <strong class="text-foreground">Réseau local :</strong>
                    <p class="text-muted mt-1">Artisans, notaires, experts de confiance</p>
                  </div>
                </div>
                <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl justify-center">
                  <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                    <CheckCircle className="w-4 h-4 text-primary" />
                  </div>
                  <div>
                    <strong class="text-foreground">Disponibilité :</strong>
                    <p class="text-muted mt-1">Réponse sous 24h garantie</p>
                  </div>
                </div>
                <div class="flex items-center gap-4 p-4 bg-primary/5 rounded-xl justify-center">
                  <div class="w-6 h-6 bg-primary/20 rounded-full flex items-center justify-center flex-shrink-0">
                    <CheckCircle className="w-4 h-4 text-primary" />
                  </div>
                  <div>
                    <strong class="text-foreground">Transparence :</strong>
                    <p class="text-muted mt-1">Frais modulables selon satisfaction</p>
                  </div>
                </div>
              </div>
            </div>
          </CardUltra>
        </div>

        <!-- CTA Section -->
        <div class="mt-20 text-center">
          <CardUltra class="group">
            <div class="text-center">
              <h3 class="text-3xl font-bold text-foreground mb-4">Vous souhaitez en savoir plus ?</h3>
              <p class="text-muted mb-8 max-w-2xl mx-auto text-lg">
                Notre équipe locale est à votre disposition pour analyser votre projet immobilier
                et vous accompagner dans vos démarches.
              </p>
              <div class="flex flex-col sm:flex-row gap-4 justify-center">
                <ButtonUltra href="/estimation" text="Estimer mon bien" />
                <ButtonUltra href="/contact" text="Nous contacter" variant="secondary" />
              </div>
            </div>
          </CardUltra>
        </div>
      </div>
    </div>
  </section>

  <!-- Newsletter Section Ultra-Moderne -->
  <section class="py-24 bg-gradient-to-br from-primary to-primary/90 text-white relative overflow-hidden">
    <!-- Motifs géométriques -->
    <div class="absolute inset-0 opacity-8">
      <div class="absolute top-10 right-10 w-32 h-32 bg-white/20 rounded-full blur-xl"></div>
      <div class="absolute bottom-10 left-10 w-24 h-24 border-2 border-white/30 rotate-45"></div>
    </div>

    <div class="container mx-auto px-4 relative z-10">
      <div class="max-w-4xl mx-auto text-center">
        <h2 class="text-4xl md:text-5xl font-light mb-6 tracking-tight">
          Recevez nos analyses<br>
          <span class="bg-gradient-to-r from-white/90 to-white/70 bg-clip-text text-transparent font-semibold">du marché savoyard</span>
